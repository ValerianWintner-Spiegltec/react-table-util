{
    "name": "react-table-util",
    "version": "0.0.21",
    "author": "Patrick Malleier <github@numero33.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/numero33/react-table-util/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/numero33/react-table-util.git"
    },
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "module": "dist/module.js",
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "tsc --watch",
        "build": "tsc",
        "test": "jest --passWithNoTests",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint src/",
        "prepare": "yarn build",
        "size": "size-limit",
        "analyze": "size-limit --why"
    },
    "peerDependencies": {
        "react": ">=16",
        "react-dom": ">=16"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn lint"
        }
    },
    "size-limit": [
        {
            "path": "dist/*.js",
            "limit": "2 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^7.0.8",
        "@testing-library/react": "^13.3.0",
        "@types/flat": "^5.0.2",
        "@types/jest": "^27.5.1",
        "@types/react": "^18.0.9",
        "@types/react-dom": "^18.0.5",
        "cross-env": "^7.0.3",
        "eslint-config-airbnb": "^19.0.4",
        "husky": "^8.0.1",
        "jest": "^28.1.0",
        "jest-environment-jsdom": "^28.1.0",
        "prettier": "^2.6.2",
        "process": "^0.11.10",
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "size-limit": "^7.0.8",
        "ts-jest": "^28.0.3",
        "tslib": "^2.4.0",
        "typescript": "^4.7.2"
    },
    "dependencies": {
        "flat": "^5.0.2"
    }
}
